# Stream Health Monitor Environment Variables
# Copy this file to .env and fill in the values

# General settings
STREAM_HEALTH_LOG_LEVEL=INFO

# Vimeo API credentials
STREAM_HEALTH_STREAMS_2_SETTINGS_API_KEY=your_vimeo_api_key
STREAM_HEALTH_STREAMS_2_SETTINGS_ACCESS_TOKEN=your_vimeo_access_token

# Email alert settings
STREAM_HEALTH_ALERTS_0_SETTINGS_SMTP_SERVER=smtp.example.com
STREAM_HEALTH_ALERTS_0_SETTINGS_SMTP_PORT=587
STREAM_HEALTH_ALERTS_0_SETTINGS_SMTP_USERNAME=your_username
STREAM_HEALTH_ALERTS_0_SETTINGS_SMTP_PASSWORD=your_password

# Webhook alert settings
STREAM_HEALTH_ALERTS_1_SETTINGS_SECRET=your_webhook_secret
